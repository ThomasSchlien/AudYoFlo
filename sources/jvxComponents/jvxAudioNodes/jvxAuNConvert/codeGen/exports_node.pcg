SECTION PROPERTIES
{	
	OUTPUTFILE_NAME = "genConvert_node";

	SECTION config
	{
		GENERATE_ENTRIES_CONFIG_FILE = "no";        // Defaults to "no" if not present
		GENERATE_ENTRIES_LINK_OBJECTS = "yes";       // Defaults to "no" if not present
		ALLOWED_STATE_MASK = {"JVX_STATE_SELECTED", "JVX_STATE_ACTIVE", "JVX_STATE_PREPARED", "JVX_STATE_PROCESSING"};
		CALLBACKS = {"set_config"};
		CALLBACK_SET_POSTHOOK = "set_config"; 

		SECTION fixed_rate_location_mode
		{
			TYPE = "JVX_DATAFORMAT_SELECTION_LIST";				
			SELECTION_LIST_NAMES = {"INPUT", "OUTPUT"};
			SELECTION_LIST_SELECTED = {"no", "yes"};
			SELECTION_LIST_EXCLUSIVE = {"no", "no"}; 
			SELECTION_TRANSLATOR_TYPE = "jvxRateLocationMode";
			SELECTION_LIST_TRANSLATORS = {"JVX_FIXED_RATE_LOCATION_INPUT", "JVX_FIXED_RATE_LOCATION_OUTPUT"};
			SELECTION_TRANSLATOR_ENUM_CLASS = "yes";
			READ_WRITE_ACCESS = "JVX_PROPERTY_ACCESS_READ_AND_WRITE_CONTENT";	
			CONTENT_DECODER_TYPE = "JVX_PROPERTY_DECODER_SINGLE_SELECTION";
		};

		SECTION buffering_mode_output
		{
			TYPE = "JVX_DATAFORMAT_SELECTION_LIST";				
			SELECTION_LIST_NAMES = {"VARIABLE", "FIXED"};
			SELECTION_LIST_SELECTED = {"yes", "no"};
			SELECTION_LIST_EXCLUSIVE = {"no", "no"}; 
			SELECTION_TRANSLATOR_TYPE = "jvxBufferingMode";
			SELECTION_LIST_TRANSLATORS = {"JVX_BUFFERING_VARIABLE", "JVX_BUFFERING_FIXED"};
			SELECTION_TRANSLATOR_ENUM_CLASS = "yes";
			READ_WRITE_ACCESS = "JVX_PROPERTY_ACCESS_READ_AND_WRITE_CONTENT";	
			CONTENT_DECODER_TYPE = "JVX_PROPERTY_DECODER_SINGLE_SELECTION";
		};

		SECTION resampler_quality
		{
			TYPE = "JVX_DATAFORMAT_SELECTION_LIST";				
			SELECTION_LIST_NAMES = {"LOW", "MEDIUM", "HIGH"};
			SELECTION_LIST_SELECTED = {"no", "yes", "no"};
			SELECTION_LIST_EXCLUSIVE = {"no", "no", "no"}; 
			SELECTION_TRANSLATOR_TYPE = "jvxResamplerQuality";
			SELECTION_LIST_TRANSLATORS = {"JVX_RESAMPLER_QUALITY_LOW", "JVX_RESAMPLER_QUALITY_MEDIUM", "JVX_RESAMPLER_QUALITY_HIGH"};
			SELECTION_TRANSLATOR_ENUM_CLASS = "yes";
			READ_WRITE_ACCESS = "JVX_PROPERTY_ACCESS_READ_AND_WRITE_CONTENT";	
			CONTENT_DECODER_TYPE = "JVX_PROPERTY_DECODER_SINGLE_SELECTION";
		};
	};

	SECTION resampling
	{
		GENERATE_ENTRIES_CONFIG_FILE = "no";        // Defaults to "no" if not present
		GENERATE_ENTRIES_LINK_OBJECTS = "yes";       // Defaults to "no" if not present
		ALLOWED_STATE_MASK = {"JVX_STATE_SELECTED", "JVX_STATE_ACTIVE", "JVX_STATE_PREPARED", "JVX_STATE_PROCESSING"};
		/*
		CALLBACKS = {"get_current_level_pre", "get_current_level_post"};
		CALLBACK_GET_PREHOOK = "get_current_level_pre"; 
		CALLBACK_GET_POSTHOOK = "get_current_level_post"; 
		*/

		SECTION factor_num
		{
			TYPE = "JVX_DATAFORMAT_SIZE";	
			INIT_SET = 1;
			READ_WRITE_ACCESS = "JVX_PROPERTY_ACCESS_READ_ONLY";
			ASSOCIATE_EXTERNAL = "yes";
		};
		SECTION factor_den
		{
			TYPE = "JVX_DATAFORMAT_SIZE";	
			INIT_SET = 1;
			READ_WRITE_ACCESS = "JVX_PROPERTY_ACCESS_READ_ONLY";	
			ASSOCIATE_EXTERNAL = "yes";
		};

		SECTION real_rate
		{
			TYPE = "JVX_DATAFORMAT_DATA";	
			INIT_SET = 1;
			READ_WRITE_ACCESS = "JVX_PROPERTY_ACCESS_READ_ONLY";				
		};
	};
};